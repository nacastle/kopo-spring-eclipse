package di.xml.n02;

public class Car {
	
	private Tire tire; //의존관계 발생
	
	
	
	
	public Car() {
		super();
		System.out.println("Car생성자 호출...");
	}
	
	

	public Car(Tire tire) {
		super();
		this.tire = tire;
	}



	// 속성 주입 (Setter 주입)
	public void setTire(Tire tire) {
		this.tire = tire;
		
	}
	
	public void prnTireBrand() {
		System.out.println("장착된 타이어: " + tire.getBrand());
	}
	

}
